{
  "compilerOptions": {
    "target": "es5",
    "rootDir": ".",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "module": "CommonJS",
    "strict": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
  }
}